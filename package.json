{
  "name": "@magento/baler",
  "private": true,
  "version": "0.0.1",
  "description": "A proof of concept to see how feasible it is to do static analysis of all JavaScript dependencies hidden within Magento 2 templates (phtml/html)",
  "main": "dist/index.js",
  "bin": {
    "baler": "bin/baler"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run format:check && jest -i",
    "format": "prettier --write $(head -1 prettier-globs)",
    "format:check": "prettier --list-different $(head -1 prettier-globs)",
    "coverage": "rm -rf coverage && jest --coverage && open coverage/lcov-report/index.html"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DrewML/baler.git"
  },
  "keywords": [
    "magento",
    "m2",
    "bundle"
  ],
  "author": "Andrew Levine",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/DrewML/balerg/issues"
  },
  "homepage": "https://github.com/DrewML/baler#readme",
  "devDependencies": {
    "@types/acorn": "^4.0.5",
    "@types/console-log-level": "^1.4.0",
    "@types/estree": "0.0.39",
    "@types/htmlparser2": "^3.10.1",
    "@types/jsesc": "^0.4.29",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.6.8",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "source-map": "^0.7.3",
    "tempy": "^0.3.0",
    "ts-jest": "^24.0.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "acorn": "^6.2.1",
    "acorn-loose": "^6.1.0",
    "chalk": "^2.4.2",
    "console-log-level": "^1.4.1",
    "esquery": "^1.0.1",
    "execa": "^2.0.4",
    "fast-glob": "^3.0.4",
    "fast-xml-parser": "^3.12.19",
    "fromentries": "^1.1.0",
    "htmlparser2": "^3.10.1",
    "jest-worker": "^24.9.0",
    "jsesc": "^2.5.2",
    "magic-string": "^0.25.3",
    "minimist": "^1.2.0",
    "ora": "^3.4.0",
    "pretty-bytes": "^5.3.0",
    "pretty-ms": "^5.0.0",
    "requirejs": "^2.3.6",
    "source-map-support": "^0.5.12",
    "terser": "^4.2.0"
  }
}
